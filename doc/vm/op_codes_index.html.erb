    <h3>OpCodes</h3>
    <ul id="op_codes">
      <% op_codes.sort.each do |op| %>
      <li><%= "<i>" if op.description.nil? || op.description.empty? %>
      <a href="op_codes/<%= op.mnemonic %>.html" title="<%= op.operation %>" target="op_code"><%= op.mnemonic %><%= " (#{op.alias})" if op.alias %></a>
      <%= "</i>" if op.description.nil? || op.description.empty? %></li>
      <% end %>
    </ul>
  </body>
